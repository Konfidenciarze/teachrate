<%- include ('partials/htmlHeader') %>
<%- include ('partials/htmlNavbar') %>	

	<div class="container">
		 <div class="row h-25">
		    <h1><center>Witaj <%= user.username %></center></h1>	<br>
			<center><button type="button" class="btn btn-teach w-25 h-50"  onclick="location.href='/rate/browse'" value="Zacznij Oceniać">Wyszukaj prowadzącego</button></center>
		 </div>
	
	<%if(votes.length!=0){%>
    <center><h5>Ostatnio ocenieni przez Ciebie prowadzący</h5></center>
	<%}else{%>
	<center><h5>Tutaj znajdą się ocenieni przez Ciebie prowadzący</h5></center>
	<%}%>
	
	
	<%for(let i =0; i < votes.length; i++){%>
		<div class="row justify-content-center searchResult align-items-center">
				<div class="col-md-auto">
					<img src="<%= votes[i].teacher.img%>" alt="photo" class="img-fluid h-75">
				</div>
				<div class="col align-items-end">
				<h2><p><%= votes[i].teacher.pref%> <%= votes[i].teacher.data%></p><span><%= votes[i].teacher.post%></span></h2>
				<h4><span><%= votes[i].teacher.faculty%></span></h4>
				<h3>
					<p>Tłumaczenie materiału: <%= votes[i].material%>/5</p>
					<p>Punktualność wykładowcy: <%= votes[i].punctual%>/5</p>
					<p>Łatwość zaliczenia: <%= votes[i].passing%>/5</p>
				</h3>
				</div>
				<div class="col align-items-end">
				<% if(votes[i].comment != '') { %>
				<h3>Komentarz: <span style="word-wrap: break-word;"> <%= votes[i].comment %> </span> </h3>
				<%}%>
				<a href="/rate/profile/<%= votes[i].teacher._id%>"><button class="btn btn-teach btn-lg">WYŚWIETL</button></a>
				</div>
		</div>
		<%}%>
	
	
	
	
	
	</div>


	
	
	
<%- include ('partials/htmlFooter') %>